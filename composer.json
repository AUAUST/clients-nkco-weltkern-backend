  "require": {
    "php": ">=8.0.0 <8.3.0",
    "getkirby/cms": "^3.9"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true
  },
  "extra": {
